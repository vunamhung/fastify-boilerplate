{
  "name": "fastify-boilerplate",
  "version": "0.9.0",
  "license": "GPL-3.0-or-later",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "schemas:compile": "json2ts -i ./src/schemas -o ./src/@types",
    "test": "NODE_ENV=test npx jest --verbose --runInBand",
    "build": "rm -rf dist && tsc --sourceMap -p ./",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "predeploy": "yarn run build",
    "deploy": "rsync -avzhe ssh --delete --delete-excluded -L --exclude-from=exclude ./ ocean:/var/www/html/"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "dotenv-flow": "^3.2.0",
    "fastify": "3.4.1",
    "fastify-autoload": "^3.1.0",
    "fastify-blipp": "3.0.0",
    "fastify-cookie": "^4.1.0",
    "fastify-cors": "^4.1.0",
    "fastify-jwt": "^2.1.3",
    "fastify-language-parser": "^3.0.0",
    "fastify-no-icon": "^4.0.0",
    "fastify-plugin": "2.3.4",
    "fastify-prettier": "^1.1.6",
    "fastify-qs": "^2.0.0",
    "fastify-rate-limit": "^4.0.2",
    "fastify-sensible": "^3.0.1",
    "fastify-static": "^3.2.0",
    "fastify-swagger": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.10.7",
    "mongoose-slug-generator": "^1.0.4",
    "pino-pretty": "^4.2.1",
    "query-string": "^6.13.2",
    "ramda": "^0.27.1",
    "source-map-support": "^0.5.10",
    "under-pressure": "^5.2.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "26.0.14",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.8",
    "@types/node": "^14.11.2",
    "@types/ramda": "^0.27.19",
    "@types/validator": "^13.1.0",
    "jest": "26.4.2",
    "json-schema-to-typescript": "^9.1.1",
    "prettier": "^2.1.2",
    "ts-jest": "26.4.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "4.0.3"
  }
}
