{
  "name": "fastify-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "start": "node ./dist/index.js",
    "prebuild": "rimraf dist",
    "build": "etsc && tsc-alias",
    "dev": "tsx watch ./src/index.ts",
    "predeploy": "npm run build",
    "deploy": "rsync -avzhe ssh --delete -L --exclude-from=exclude ./ fastify:/root/api/"
  },
  "dependencies": {
    "@fastify/autoload": "^5.9.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/sensible": "^5.6.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@fastify/under-pressure": "^8.5.0",
    "@fastify/websocket": "^10.0.1",
    "@scalar/fastify-api-reference": "^1.24.16",
    "ajv-keywords": "5.1.0",
    "axios": "^1.6.4",
    "bcryptjs": "^2.4.3",
    "dotenv-flow": "^4.1.0",
    "envalid": "8.0.0",
    "fast-rbac": "^2.0.1",
    "fastify": "^4.28.0",
    "fastify-axios": "^1.2.8",
    "fastify-ip": "^1.0.0",
    "fastify-plugin": "4.5.1",
    "nanoid": "^3.3.7",
    "patch-package": "^8.0.0",
    "pino-pretty": "^11.2.1",
    "ramda": "^0.30.1",
    "ramda-adjunct": "^5.0.1",
    "redis": "^4.6.12",
    "server-health": "^5.3.0",
    "slugify": "^1.6.6",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.24.7",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.2",
    "@types/ramda": "^0.30.0",
    "@types/validator": "^13.12.0",
    "@types/ws": "^8.5.10",
    "esbuild": "^0.21.5",
    "esbuild-node-tsc": "2.0.5",
    "fastify-type-provider-zod": "^1.2.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5"
  },
  "engines": {
    "npm": ">=9.8.1",
    "node": ">=18.18"
  }
}
