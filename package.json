{
  "name": "fastify-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "start": "node ./dist/index.js",
    "prebuild": "rimraf dist",
    "build": "etsc && tsc-alias",
    "dev": "tsx watch ./src/index.ts",
    "predeploy": "npm run build",
    "deploy": "rsync -avzhe ssh --delete -L --exclude-from=exclude ./ fastify:/root/api/"
  },
  "dependencies": {
    "@fastify/autoload": "5.7.1",
    "@fastify/cookie": "9.1.0",
    "@fastify/cors": "8.4.0",
    "@fastify/helmet": "11.1.1",
    "@fastify/jwt": "7.2.1",
    "@fastify/sensible": "5.3.0",
    "@fastify/swagger": "8.10.1",
    "@fastify/swagger-ui": "^1.9.3",
    "@fastify/under-pressure": "8.3.0",
    "@fastify/websocket": "^8.2.0",
    "ajv-keywords": "5.1.0",
    "axios": "1.5.1",
    "bcryptjs": "^2.4.3",
    "dotenv-flow": "4.0.0",
    "envalid": "8.0.0",
    "fastify": "4.23.2",
    "fastify-axios": "1.2.6",
    "fastify-plugin": "4.5.1",
    "nanoid": "5.0.1",
    "patch-package": "^8.0.0",
    "pino-pretty": "^10.2.0",
    "ramda": "0.29.0",
    "ramda-adjunct": "4.1.1",
    "redis": "^4.6.10",
    "slugify": "^1.6.6",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.23.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/node": "20.8.0",
    "@types/ramda": "0.29.3",
    "@types/validator": "^13.11.2",
    "@types/ws": "^8.5.6",
    "esbuild": "0.19.4",
    "esbuild-node-tsc": "2.0.5",
    "fastify-type-provider-zod": "^1.1.9",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "tsc-alias": "1.8.8",
    "tsx": "^3.13.0",
    "typescript": "5.2.2"
  },
  "engines": {
    "npm": ">=9.8.1",
    "node": ">=18.18"
  }
}
