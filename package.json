{
  "name": "fastify-boilerplate",
  "version": "0.9.0",
  "license": "GPL-3.0-or-later",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "NODE_ENV=test npx jest --runInBand",
    "build": "rm -rf dist && tsc --sourceMap -p ./",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "predeploy": "yarn run build",
    "deploy": "rsync -avzhe ssh --delete -L --exclude-from=exclude ./ cheap:/root/fastify/"
  },
  "dependencies": {
    "aws-sdk": "2.891.0",
    "bcryptjs": "^2.4.3",
    "camelcase-keys": "^6.2.2",
    "crypto": "^1.0.1",
    "deep-email-validator": "0.1.18",
    "dotenv-flow": "^3.2.0",
    "ejs": "^3.1.5",
    "fastify": "3.15.0",
    "fastify-autoload": "3.7.1",
    "fastify-blipp": "3.0.0",
    "fastify-compress": "3.4.2",
    "fastify-cookie": "5.3.0",
    "fastify-cors": "5.2.0",
    "fastify-guard": "1.2.0",
    "fastify-helmet": "5.3.1",
    "fastify-jwt": "2.4.0",
    "fastify-multer": "^2.0.2",
    "fastify-nextjs": "5.4.0",
    "fastify-no-icon": "^4.0.0",
    "fastify-plugin": "3.0.0",
    "fastify-prettier": "1.1.7",
    "fastify-rate-limit": "5.5.0",
    "fastify-response-caching": "^0.0.3",
    "fastify-sensible": "3.1.1",
    "fastify-static": "4.0.1",
    "fastify-swagger": "4.7.0",
    "mailgun-js": "^0.22.0",
    "mongoose": "5.11.20",
    "mongoose-bcrypt": "^1.9.0",
    "mongoose-deep-populate": "^3.2.0",
    "mongoose-delete": "^0.5.3",
    "mongoose-paginate": "^5.0.3",
    "mongoose-slug-generator": "^1.0.4",
    "omit-empty": "^1.0.0",
    "password-validator": "^5.1.1",
    "pino-pretty": "4.7.1",
    "point-of-view": "4.14.0",
    "ramda": "^0.27.1",
    "rand-token": "^1.0.1",
    "sharp": "0.28.1",
    "source-map-support": "^0.5.10",
    "under-pressure": "5.6.0",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "26.0.22",
    "@types/mailgun-js": "0.22.11",
    "@types/mongodb": "3.6.12",
    "@types/node": "14.14.41",
    "@types/ramda": "0.27.40",
    "@types/validator": "13.1.3",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.4"
  }
}
