{
  "name": "fastify-boilerplate",
  "version": "0.9.0",
  "license": "GPL-3.0-or-later",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "NODE_ENV=test npx jest --runInBand",
    "build": "rm -rf dist && tsc --sourceMap -p ./",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "predeploy": "yarn run build",
    "deploy": "rsync -avzhe ssh --delete -L --exclude-from=exclude ./ ocean:/var/www/html/"
  },
  "dependencies": {
    "aws-sdk": "2.820.0",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "deep-email-validator": "0.1.18",
    "dotenv-flow": "^3.2.0",
    "ejs": "^3.1.5",
    "fastify": "3.9.2",
    "fastify-autoload": "3.4.0",
    "fastify-blipp": "3.0.0",
    "fastify-compress": "3.4.1",
    "fastify-cookie": "5.1.0",
    "fastify-cors": "5.1.0",
    "fastify-guard": "1.2.0",
    "fastify-helmet": "^5.0.3",
    "fastify-jwt": "2.2.0",
    "fastify-multer": "^2.0.2",
    "fastify-nextjs": "5.1.0",
    "fastify-no-icon": "^4.0.0",
    "fastify-plugin": "3.0.0",
    "fastify-prettier": "1.1.7",
    "fastify-qs": "3.0.0",
    "fastify-rate-limit": "4.1.0",
    "fastify-response-caching": "^0.0.3",
    "fastify-sensible": "3.1.0",
    "fastify-static": "3.3.0",
    "fastify-swagger": "3.5.0",
    "mailgun-js": "^0.22.0",
    "mongoose": "5.11.9",
    "mongoose-deep-populate": "^3.2.0",
    "mongoose-slug-generator": "^1.0.4",
    "password-validator": "^5.1.1",
    "pino-pretty": "4.3.0",
    "point-of-view": "4.7.0",
    "query-string": "6.13.8",
    "ramda": "^0.27.1",
    "rand-token": "^1.0.1",
    "sharp": "0.27.0",
    "source-map-support": "^0.5.10",
    "under-pressure": "5.4.0",
    "validator": "13.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "26.0.19",
    "@types/mailgun-js": "0.22.11",
    "@types/mongodb": "3.6.3",
    "@types/node": "14.14.17",
    "@types/query-string": "^6.3.0",
    "@types/ramda": "0.27.34",
    "@types/validator": "13.1.2",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  }
}
